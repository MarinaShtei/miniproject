<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="pane" prefHeight="450.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="gui.OperationsWorkerFrameController">
   <children>
      <Label fx:id="lblWelcome" layoutX="112.0" layoutY="28.0" text="Welcome" textFill="#e10000">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </Label>
      <Button fx:id="btnExit" layoutX="716.0" layoutY="395.0" mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="60.0" text="Exit" textFill="RED">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnUpdateStock" layoutX="150.0" layoutY="228.0" mnemonicParsing="false" onAction="#updateStocks" prefHeight="50.0" prefWidth="500.0" text="Update vending machines stock">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnViewVendingMachines" layoutX="150.0" layoutY="146.0" mnemonicParsing="false" onAction="#ViewVendingMachinesDetails" prefHeight="50.0" prefWidth="500.0" text="View vending machines to restock">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
