<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.RegionManagerFrameController">
   <children>
      <Label fx:id="lblWelcome" layoutX="102.0" layoutY="18.0" text="Welcome" textFill="#e10000">
         <font>
            <Font name="Arial" size="16.0" />
         </font></Label>
      <Button fx:id="btnUpdateThresholdLevel" layoutX="150.0" layoutY="84.0" mnemonicParsing="false" onAction="#updateThresholdLevel" prefHeight="50.0" prefWidth="500.0" text="Update threshold level">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnExit" layoutX="706.0" layoutY="385.0" mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="60.0" text="Exit" textFill="RED">
         <font>
            <Font size="16.0" />
         </font></Button>
      <Button fx:id="btnRestockMessage" layoutX="150.0" layoutY="162.0" mnemonicParsing="false" onAction="#restockMessageToWorker" prefHeight="50.0" prefWidth="500.0" text="Restock message to worker">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnViewReports" layoutX="150.0" layoutY="237.0" mnemonicParsing="false" onAction="#viewReports" prefHeight="50.0" prefWidth="500.0" text="View reports">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnViewRegistrationRequests" layoutX="150.0" layoutY="317.0" mnemonicParsing="false" onAction="#viewRegistrationRequests" prefHeight="50.0" prefWidth="500.0" text="View Registration Requests">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
