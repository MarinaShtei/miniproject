<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.InstallFrameController">
   <children>
      <RadioButton fx:id="radioBtnOL" layoutX="123.0" layoutY="200.0" mnemonicParsing="false" onAction="#chooseOL" text="OL - remote">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="radioBtnEK" layoutX="123.0" layoutY="248.0" mnemonicParsing="false" onAction="#chooseEK" text="EK - local">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </RadioButton>
      <Button fx:id="btnInstall" layoutX="134.0" layoutY="360.0" mnemonicParsing="false" onAction="#install" prefHeight="35.0" prefWidth="70.0" text="Install">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Label fx:id="lblMsg" layoutX="143.0" layoutY="297.0" text="*choose vending machine location:" textFill="#d30e0e">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <ComboBox fx:id="cmbBoxVendingMachine" layoutX="401.0" layoutY="298.0" prefWidth="150.0" />
   </children>
</AnchorPane>
